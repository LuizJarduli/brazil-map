{
  "name": "brazil-map",
  "version": "1.1.8",
  "description": "A simple interactive svg Brazil map web component",
  "main": "./dist/brazil-map.umd.js",
  "module": "./dist/brazil-map.es.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/brazil-map.es.js",
      "require": "./dist/brazil-map.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "npx vite build",
    "build:watch": "npx tsc --watch",
    "publish:package": "npm run build && npx standard-version && git push --follow-tags origin master && npm publish"
  },
  "contributors": [
    "LuizJarduli"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LuizJarduli/brazil-map.git"
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "keywords": [
    "Brazil",
    "Brasil",
    "svg",
    "map",
    "meme",
    "lit",
    "webcomponents",
    "typescript"
  ],
  "author": "Luiz Miguel luizjarduli@gmail.com",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/LuizJarduli/brazil-map/issues"
  },
  "homepage": "https://github.com/LuizJarduli/brazil-map#readme",
  "dependencies": {
    "lit": "~3.2.1"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "~15.2.3",
    "@rollup/plugin-terser": "~0.4.4",
    "@types/node": "~20.11.20",
    "@typescript-eslint/eslint-plugin": "~6.21.0",
    "@web/rollup-plugin-html": "~2.3.0",
    "eslint": "~9.27.0",
    "eslint-config-prettier": "~9.1.0",
    "eslint-config-standard-with-typescript": "~43.0.1",
    "eslint-plugin-import": "~2.29.1",
    "eslint-plugin-lit": "~1.11.0",
    "eslint-plugin-n": "~16.6.2",
    "eslint-plugin-promise": "~6.1.1",
    "rollup-plugin-minify-html-literals": "~1.2.6",
    "rollup-plugin-summary": "~2.0.0",
    "terser": "~5.37.0",
    "ts-lit-plugin": "~2.0.2",
    "typescript": "~5.7.2",
    "vite": "~6.2.0"
  }
}
